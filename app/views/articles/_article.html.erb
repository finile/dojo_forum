 <%= form_for @article do |f| %>
  
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>
  
  <%= f.label :content, "Contnet" %>
  <%= f.text_area :content %>
    
  <%= f.label :image, "File" %>
  <%= f.file_field :image %>
  
  <%= f.submit 'save' %>
  <%= f.submit 'submit' %>
  
  
<% end %>



